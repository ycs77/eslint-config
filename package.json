{
  "name": "@ycs77/eslint-config",
  "type": "module",
  "version": "6.2.0",
  "description": "Lucas Yang's ESLint config",
  "author": "Lucas Yang <yangchenshin77@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ycs77/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/ycs77/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run gen && tsdown --dts",
    "stub": "tsdown",
    "dev:watch": "tsdown --watch",
    "dev:inspector": "eslint-config-inspector --no-open",
    "build:inspector": "eslint-config-inspector build",
    "lint": "eslint .",
    "gen": "tsx scripts/typegen.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "code-check": "npm run lint && npm run typecheck && npm run test -- --run",
    "prepack": "npm run build",
    "release": "bumpp --commit \"Release v%s\" && npm publish"
  },
  "peerDependencies": {
    "eslint": ">=9.10.0",
    "eslint-plugin-astro-explicit-wrapper": "^1.0.3"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-astro-explicit-wrapper": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-config": "~6.7.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint-plugin-antfu": "^3.1.1",
    "local-pkg": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.4.2",
    "@types/node": "^22.19.1",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "eslint-flat-config-utils": "^2.1.4",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-astro-explicit-wrapper": "^1.0.5",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-vue": "^10.6.2",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-typegen": "^2.3.0",
    "execa": "^9.6.1",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.18.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "vue": "^3.5.26"
  }
}
