{
  "name": "@ycs77/eslint-config",
  "type": "module",
  "version": "5.0.3",
  "description": "Lucas Yang's ESLint config",
  "author": "Lucas Yang <yangchenshin77@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ycs77/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/ycs77/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run gen && tsdown --dts",
    "stub": "tsdown",
    "dev:watch": "tsdown --watch",
    "dev:inspector": "eslint-config-inspector --no-open",
    "build:inspector": "eslint-config-inspector build",
    "lint": "eslint .",
    "gen": "tsx scripts/typegen.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "code-check": "npm run lint && npm run typecheck && npm run test -- --run",
    "prepack": "npm run build",
    "release": "bumpp --commit \"Release v%s\" && npm publish"
  },
  "peerDependencies": {
    "eslint": ">=9.10.0",
    "eslint-plugin-astro-explicit-wrapper": "^1.0.3"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-astro-explicit-wrapper": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-config": "~5.2.2",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint-plugin-antfu": "^3.1.1",
    "local-pkg": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.18.3",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "eslint-flat-config-utils": "^2.1.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-astro-explicit-wrapper": "^1.0.3",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.1",
    "tsdown": "^0.15.1",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.21"
  }
}
